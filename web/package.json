{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@mui/material": "5.11.7",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.2.5",
    "@types/node": "16.18.11",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.7.0",
    "react-scripts": "5.0.1",
    "react-use": "17.4.0",
    "typescript": "4.9.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "yarn lint:eslint && yarn lint:tsc",
    "lint:eslint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "lint:tsc": "tsc --noEmit",
    "format": "prettier --write --list-different 'src/**/*.{js,jsx,ts,tsx,css}'",
    "format-check": "prettier --list-different 'src/**/*.{js,jsx,ts,tsx,css}'",
    "ci:lint": "yarn lint:eslint --format gitlab && yarn lint:tsc",
    "ci:test": "yarn test --ci --coverage --coverageDirectory=coverage --coverageReporters=text --coverageReporters=cobertura --reporters=default --reporters=jest-junit"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "warn",
        { 
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_",
          "caughtErrorsIgnorePattern": "^_"
        }
      ],
      "prettier/prettier": "warn",
      "react/jsx-uses-react": "off",
      "react/react-in-jsx-scope": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-formatter-gitlab": "4.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest-junit": "15.0.0",
    "prettier": "2.8.2"
  }
}
